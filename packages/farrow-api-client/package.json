{
  "name": "farrow-api-client",
  "version": "1.10.11",
  "description": "A type-friendly BFF framework",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": "./dist/*.js",
    "./esm/*": "./dist/*.js"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "jest",
    "test:coverage": "jest --collectCoverage --coverage"
  },
  "files": [
    "src",
    "dist",
    "esm"
  ],
  "keywords": [
    "Web Framework",
    "TypeScript",
    "Functional Programming"
  ],
  "author": "Jade Gu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucifier129/farrow.git"
  },
  "peerDependencies": {
    "farrow-pipeline": "^1.10.7"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "farrow-api-server": "^1.10.9",
    "farrow-pipeline": "^1.10.7",
    "jest": "^27.2.2",
    "ts-jest": "^27.0.5"
  },
  "dependencies": {
    "dataloader": "^2.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "setimmediate": "^1.0.5",
    "tslib": "^2.1.0"
  }
