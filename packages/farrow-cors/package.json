{
  "name": "farrow-cors",
  "version": "1.10.9",
  "description": "Cross-Origin Resource Sharing(CORS) for farrow-http",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": "./dist/*.js",
    "./esm/*": "./dist/*.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "Web Framework",
    "TypeScript",
    "Functional Programming",
    "cors"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "jest",
    "test:coverage": "jest --collectCoverage --coverage"
  },
  "author": "Jade Gu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucifier129/farrow.git"
  },
  "peerDependencies": {
    "farrow-http": "^1.8.0",
    "farrow-pipeline": "^1.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/jest": "^27.0.1",
    "@types/supertest": "^2.0.10",
    "farrow-http": "^1.10.9",
    "farrow-pipeline": "^1.10.6",
    "jest": "^27.2.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "tslib": "^2.1.0"
  }
}
